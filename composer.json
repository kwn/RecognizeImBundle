{
    "name":        "kwn/recognizeim-bundle",
    "type":        "symfony-bundle",
    "description": "Symfony2 bundle that provides Recognize.im PHP client. Allows image recognizing, includes REST and SOAP API.",
    "homepage":    "https://github.com/kwn/RecognizeImBundle",
    "license":     "MIT",
    "keywords":    ["recognizeim", "recognize.im", "image", "reconizing", "recognize", "symfony", "symfony2", "bundle"],
    "authors": [
        {
            "name":     "Karol Wnuk",
            "email":    "k.wnuk@ascetic.pl",
            "homepage": "https://github.com/kwn",
            "role":     "developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "kwn/recognizeim": "dev-master",
        "symfony/framework-bundle": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": { "Kwn\\RecognizeImBundle": "" }
    },
    "target-dir": "Kwn/RecognizeImBundle"
}
